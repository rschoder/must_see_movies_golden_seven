<h1> Add A New Movie </h1>

<form action="http://localhost:3000/update_movie/:id">

  <label> Title</label>
  <input type="text" name="title">

  <label> Year</label>
  <input type="integer" name="year">

  <label> Duration</label>
  <input type="integer" name="duration">

  <label> Description</label>
  <input type="text" name="description">

    <label> URL</label>
  <input type="string" name="image_url">


  <button> Create Movie </button>
</form>

<a href=" /movies"> Go Back</a>
